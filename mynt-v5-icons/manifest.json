{
  "manifest_version": 3,
  "name": "Material You New Tab",
  "version": "5.0.0",
  "description": "Material You New Tab - Google Apps, AI Tools & Chrome Bookmarks",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "bookmarks"],
  "host_permissions": [
    "https://www.google.com/*",
    "https://fonts.gstatic.com/*",
    "https://ssl.gstatic.com/*",
    "https://www.gstatic.com/*",
    "https://wttr.in/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
